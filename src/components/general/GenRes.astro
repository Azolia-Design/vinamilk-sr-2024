---
import { Image } from "astro:assets";
import genResImgMB from '../../assets/genResImgMB.svg';
import genResContent from '../../assets/genResContent.svg?raw';
---
<section class="gen-res">
    <div class="container">
        <div class="ex-wrap">
            <div class="ex-content">
                <Fragment set:html={genResContent} />
            </div>
        </div>
        <div class="ex-wrap ex-mb">
            <div class="ex-img" data-aos="fade-up">
                <Image src={genResImgMB} alt="" />
            </div>
        </div>
    </div>
</section>
<style lang="scss">
    .gen-res {
        background-color: var(--cl-blue);
    }
</style>